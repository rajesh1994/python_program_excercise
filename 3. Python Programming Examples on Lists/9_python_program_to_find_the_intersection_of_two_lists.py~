"""
Problem Statement : Python Program to Find the Intersection of Two Lists
"""

print ("Python Program to Find the Intersection of Two Lists")

def Intersection(l1, l3):
    return list(set(l1) & set(l3))
l1 = []
n1 = int(input("Enter the number of elements for list1:"))

for i in range(0, n1):
    l2 = int(input("Enter the elements for list1:"))
    l1.append(l2)

l3 = []
n2 = int(input("Enter the number of elements for list2:"))

for j in range(0, n2):
    l4 = int(input("Enter the elements for list2:"))
    l3.append(l4)
print ("List1 & List2 are respectively", l1, "&", l3)
print ("Intersection of Two Lists", Intersection(l1, l3))
