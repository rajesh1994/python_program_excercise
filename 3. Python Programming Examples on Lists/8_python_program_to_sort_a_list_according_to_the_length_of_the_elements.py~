"""
Problem Statement : Python Program to Find the Union of two Lists
"""

print ("Problem Statement : Python Program to Find the Union of two Lists")

def Union(l1, l3):
    final_list = l1 + l3
    return final_list

n1 = int(input("Enter the number of elements:"))
l1 = []

for i in range(0, n1):
    l2 = int(input("Enter a element:"))
    l1.append(l2)
    
n2 = int(input("Enter the number of elemnts:"))
l3 = []

for j in range(0, n2):
    l4 = int(input("Enter a element:"))
    l3.append(l4)
print (Union(l1, l3))
